<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp" />
            <solid android:color="@color/list_cell_bg_clocked" />
            <stroke android:width="0.25dp" android:color="@color/list_cell_bg" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp" />
            <solid android:color="@color/normal" />
            <stroke android:width="0.25dp" android:color="@color/list_cell_bg" />
        </shape>
    </item>
</selector>